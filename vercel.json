{
    "builds": [
        {
            "src": "wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "wsgi.py"
        }
    ],
    "static": [
        {
          "src": "staticfiles",
          "dest": "staticfiles"
        }
      ]
}